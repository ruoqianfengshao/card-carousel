{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowJs": true,
    "target": "es2015",
    "module": "esnext",
    "strict": false,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny":false,
    "allowSyntheticDefaultImports": true,
